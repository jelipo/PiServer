<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="corn/dao/Nosql2SqlDao">



	<insert id="insertparame" useGeneratedKeys="true" keyProperty="id"
		parameterType="corn.pojo.Parame">
		insert
		information(temperature,humidity,year,month,day,hour,minute,second)
		values
		(#{temperature}, #{humidity}, #{year},#{month},#{day},#{hour},#{minute},#{second})
	</insert>



</mapper>