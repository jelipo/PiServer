<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Server.dao.InfDao">

	<select id="select" parameterType="Parame" resultType="HashMap">
		select *
		from information where year=#{year} ;
	</select>

	<insert id="insertparame" useGeneratedKeys="true" keyProperty="id"
		parameterType="Parame">
		insert
		information(temperature,humidity,year,month,day,hour,minute,second)
		values
		(#{temperature}, #{humidity}, #{year},#{month},#{day},#{hour},#{minute},#{second})
	</insert>

	<insert id="insertparameList" useGeneratedKeys="true"
		keyProperty="id" parameterType="java.util.List">
		insert
		information(temperature,humidity,year,month,day,hour,minute,second)
		values
		<foreach collection="list" index="index" item="item" open="("
			separator="," close=")">
			#{item.temperature}, #{item.humidity}, #{item.year},#{item.month},
			#{item.day},#{item.hour},#{item.minute},#{item.second}
		</foreach>

	</insert>

</mapper>